<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="maintenance.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//app[@name='maintenance']" position="inside">
                <block title="Sign Oca" name="maintenance_sign_oca_settings_container">
                    <setting>
                        <field name="maintenance_equipment_sign_oca_template_id" />
                    </setting>
                </block>
            </xpath>
        </field>
    </record>
</odoo>

[Thumbnailer Entry]
Exec=sh -e -c 'I="$1"; O="$2"; S="$3"; shift 3; "$@" --no-progress --num-workers=1 --always-write --no-compress --ldr-format=png --hdr-format=skip --hdr-to-ldr --no-animate --output-file="$O".png --max-resolution="$S" --closest-resolution --frames=0 --faces=0 --slices=0 -- "$I"; [ -f "$O".png ]; cat -- "$O".png > "$O"; rm -f "$O".png' no_vtf-thumbnailer %i %o %s python3 -m no_vtf
MimeType=application/x-vtf;

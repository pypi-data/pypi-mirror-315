for $c in (rx1day_19712100_tnst_1x1km_year_cordexadj_qdm)
return encode(
   coverage count_incr
      over $px X(imageCrsDomain($c[X(4400000:4420000)], X)),
           $py Y(imageCrsDomain($c[Y(2600000:2620000)], Y))
      values
           condense +
           over $m M(imageCrsDomain($c, M))
           where ( avg($c[X($px), Y($py), year(2080:2100), rcp(4.5), M($m)]) -
                   avg($c[X($px), Y($py), year(2060:2080), rcp(4.5), M($m)])) > 0
           using 1,
   "png")
